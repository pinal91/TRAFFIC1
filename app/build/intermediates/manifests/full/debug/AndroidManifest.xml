<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.traffic"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.CAMERA" />

    <android:uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <android:uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <android:uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.traffic.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.traffic.Loginpage"
            android:label="@string/title_activity_loginpage" />
        <activity
            android:name="com.example.traffic.Registrationpage"
            android:label="@string/title_activity_registrationpage" />
        <activity
            android:name="com.example.traffic.HomePage"
            android:label="@string/title_activity_home_page" />
        <activity
            android:name="com.example.traffic.FileFine"
            android:label="@string/title_activity_file_fine" />
        <activity
            android:name="com.example.traffic.FindOwner"
            android:label="@string/title_activity_find_owner" />
        <activity android:name="com.example.traffic.CameraTestActivity" />
        <activity android:name="com.example.traffic.Searchdetail" />
        <activity
            android:name="com.example.traffic.SearchQrCode"
            android:label="@string/title_activity_search_qr_code" />
        <activity android:name="com.example.traffic.History" />
        <activity
            android:name="com.example.traffic.Add_Traffic"
            android:label="@string/title_activity_add__traffic" />
        <activity
            android:name="com.example.traffic.Traffic_page"
            android:label="@string/title_activity_traffic_page" />
        <activity android:name="com.example.traffic.MyLocation" />
        <activity
            android:name="com.example.traffic.SettingPage"
            android:label="@string/title_activity_setting_page" />
        <activity
            android:name="com.example.traffic.BreakdownPage"
            android:label="@string/title_activity_breakdown_page" />
        <activity android:name="com.example.traffic.MailActivity" />
    </application>

</manifest>